{
    "name": "grechanyuk/metrolist",
    "description": "List of metro stations and lines",
    "license": "MIT",
    "authors": [
        {
            "name": "Egor G.",
            "email": "xorolllo@hotmail.com"
        }
    ],
    "homepage": "https://github.com/grechanyuk/metrolist",
    "keywords": ["Laravel", "MetroList"],
    "require": {
        "illuminate/support": "~5",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Grechanyuk\\MetroList\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Grechanyuk\\MetroList\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Grechanyuk\\MetroList\\MetroListServiceProvider"
            ],
            "aliases": {
                "MetroList": "Grechanyuk\\MetroList\\Facades\\MetroList"
            }
        }
    }
}
